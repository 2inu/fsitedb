<h1>Listing places</h1>

<table>
  <tr>
    <th>Pl place</th>
    <th>Pl place cd</th>
    <th>Pl name</th>
    <th>Pl panchayat</th>
    <th>Pl dist</th>
    <th>Pl create from</th>
    <th>Pl create date</th>
    <th>Pl create by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.pl_place_id %></td>
    <td><%= place.pl_place_cd %></td>
    <td><%= place.pl_name %></td>
    <td><%= place.pl_panchayat_id %></td>
    <td><%= place.pl_dist_id %></td>
    <td><%= place.pl_create_from %></td>
    <td><%= place.pl_create_date %></td>
    <td><%= place.pl_create_by %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
