<h1>Listing product_groups</h1>

<table>
  <tr>
    <th>Pg group cd</th>
    <th>Pg group name</th>
    <th>Pg create from</th>
    <th>Pg create date</th>
    <th>Pg create by</th>
    <th>Pg modify date</th>
    <th>Pg modify by</th>
    <th>Pg del by</th>
    <th>Pg del date</th>
    <th>Pg del flg</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @product_groups.each do |product_group| %>
  <tr>
    <td><%= product_group.pg_group_cd %></td>
    <td><%= product_group.pg_group_name %></td>
    <td><%= product_group.pg_create_from %></td>
    <td><%= product_group.pg_create_date %></td>
    <td><%= product_group.pg_create_by %></td>
    <td><%= product_group.pg_modify_date %></td>
    <td><%= product_group.pg_modify_by %></td>
    <td><%= product_group.pg_del_by %></td>
    <td><%= product_group.pg_del_date %></td>
    <td><%= product_group.pg_del_flg %></td>
    <td><%= link_to 'Show', product_group %></td>
    <td><%= link_to 'Edit', edit_product_group_path(product_group) %></td>
    <td><%= link_to 'Destroy', product_group, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

